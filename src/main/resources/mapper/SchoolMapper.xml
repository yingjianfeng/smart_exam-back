<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ecjtu.exam.dao.ISchoolDao">

    <select id="qryAll" resultType="School">
		select *
		from school
	</select>
	<select id="qryById" parameterType="int" resultType="School">
		select * from school where id=#{id}
	</select>

</mapper>